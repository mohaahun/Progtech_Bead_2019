<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url('/cards/ownbackground.png')" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ScoreController">
   <children>
      <Button fx:id="closebutton" mnemonicParsing="false" text="Exit">
            </Button>
      <TableView fx:id="ScoreTable" layoutX="200.0" layoutY="4.0" prefHeight="393.0" prefWidth="200.0">
        <columns>
            <TableColumn prefWidth="75.0" fx:id="NameColumn" />
            <TableColumn prefWidth="75.0" fx:id="StepColumn" sortable="true" sortType="ASCENDING"/>
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</Pane>
